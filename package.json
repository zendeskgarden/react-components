    "eslint": "9.39.2",
    "execa": "9.6.1",
    "inquirer": "13.2.2",
    "jsdom": "27.4.0",
    "lerna": "9.0.3",
    "ora": "9.1.0",
    "oxfmt": "0.26.0",
    "oxlint": "1.41.0",
    "postcss-styled-syntax": "0.7.1",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "storybook": "10.2.1",
