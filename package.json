    "typescript": "5.9.3",
    "vite": "npm:rolldown-vite@7.3.1",
    "vite-plugin-svgr": "4.5.0",
    "vitest": "4.0.18"
  },
  "peerDependencies": {
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0 || ^19.0.0",
    "react-dom": "^16.8.0 || ^17.0.0 || ^18.0.0 || ^19.0.0"
  },
  "packageManager": "pnpm@10.28.2",
  "pnpm": {
    "overrides": {
      "vite": "npm:rolldown-vite@7.3.1"
    }
  }
